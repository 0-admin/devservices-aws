{
    "Version":"2012-10-17",
    "Statement": [
        {
            "Sid": "AllowPublicListAccessToBugzillaCartons",
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Principal": "*",
            "Resource": "arn:aws:s3:::moz-devservices-bmocartons"
        },
        {
            "Sid": "AllowPublicAccessToBugzillaCartons",
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:GetObjectVersion"
            ],
            "Principal": "*",
            "Resource": "arn:aws:s3:::moz-devservices-bmocartons/*"
        }
    ]
}
