{
    "Version":"2012-10-17",
    "Statement": [
        {
            "Sid": "AllowListAccessToBugzillaCartons",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket"
            ],
            "Resource": "arn:aws:s3:::moz-devservices-bmocartons"
        },
        {
            "Sid": "AllowFullAccessToBugzillaCartons",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:GetObjectVersion",
                "s3:DeleteObject",
                "s3:DeleteObjectVersion"
            ],
            "Resource": "arn:aws:s3:::moz-devservices-bmocartons/*"
        }
    ]
}
